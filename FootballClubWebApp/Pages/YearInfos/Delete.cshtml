@page
@model FootballClubWebApp.Pages.YearInfos.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1 class="text-center heading">Delete Year Details</h1>
<div>
    <h4>Are You Sure to Remove This Year Details?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.YearInfo.YearName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.YearInfo.YearName)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="YearInfo.Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>
